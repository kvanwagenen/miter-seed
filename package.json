{
    "name": "miter-seed",
    "version": "0.0.1",
    "description": "A sensible startup project for the Miter framework",
    "scripts": {
        "build": "npm run clean-dist -s && tsc",
        "start": "npm run build -s && node ./dist/index.js",
        "watch": "npm run clean-dist -s && tsc --watch",
        "clean-dist": "rm -rf ./dist",
        "generate": "./node_modules/.bin/miter generate",
        "migrate": "./node_modules/.bin/miter migrate"
    },
    "contributors": [
        "Brandon Slade <brandonyoyoslade@gmail.com>"
    ],
    "license": "ISC",
    "main": "./dist/index.js",
    "dependencies": {
        "@types/config": "^0.0.32",
        "@types/node": "^7.0.12",
        "config": "^1.25.1",
        "miter": "^0.1.4",
        "miter-cli": "^0.1.3",
        "ts-task": "^0.0.1"
    },
    "keywords": [
        "miter",
        "miter-framework",
        "miter-seed"
    ],
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/miter-framework/miter-seed.git"
    },
    "bugs": {
        "url": "https://github.com/miter-framework/miter-seed/issues"
    },
    "homepage": "https://github.com/miter-framework/miter-seed#readme"
}
